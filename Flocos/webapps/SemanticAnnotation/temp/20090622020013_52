//<![CDATA[
var pageInit = function() {


// Initialize popup profiles.
YAHOO.forumsPopProfile.init();

// If a search, tint keywords.

// Tint incoming searches from google.
// http://www.google.com/search?hl=en&q=future+honda+odyssey&btnG=Search
if (document.referrer.indexOf('http://www.google.') > -1) {
var query;
if ((query = document.referrer.match(/q=([^& ;]+)/)) != null) {
var kw = query[1];
kw = kw.replace(/\+/g, "%20");
YAHOO.forumsTintText.tint(kw);
}
}

// Tint incoming searches from yahoo.
// http://search.yahoo.com/search?p=diesel+vs+gasoline
if (document.referrer.indexOf('http://www.yahoo.com') > -1) {
var query;
if ((query = document.referrer.match(/p=([^& ;]+)/)) != null) {
var kw = query[1];
kw = kw.replace(/\+/g, "%20");
YAHOO.forumsTintText.tint(kw);
}
}

};

YAHOO.util.Event.addListener( window, "load", pageInit ); 

//]]>
</script>


