<script language="javascript" type="text/javascript">
//<![CDATA[

// Change the current location to the select object url.
function jumpToBoard(targ, selObj, restore) {
    var url = selObj.options[selObj.selectedIndex].value;

    // Skip no-ops.
    if (url == "#") { return; }

    // Load the new page.
    eval(targ + ".location='" + url + "'");
    if (restore) { selObj.selectedIndex=0; }
}

// Open a narrow, non-tabbar window.
function popWindow(strFile) {
    window.open(strFile, "pop", "height=400,width=525,scrollbars,resizable,status,menubar");}

// Open a window with a full tabbar width.
function popWinTwo(strFile) {
    window.open(strFile, "pop", "height=400,width=800,scrollbars,resizable,status,menubar");}

// Set tid value in cookie.
function setTID(value){
    var today = new Date();
    var expdate = new Date(today.getTime() +  30000); //expire in 30 seconds
    document.cookie="tid=" + escape(value) + "; expires=" + expdate.toGMTString() + "; path=/; domain=.edmunds.com";
}

function ed_genDartFile(adtype) {
if (adtype == "spotlight" && ed_req_model) {
return(ed_req_make + ".forums.dart");
} else  {
return("forums.dart");
}
}

function ed_genDartParams(adtype) {
var params = "";
if (adtype == "spotlight" && ed_req_model) {
params += "mdl=" + ed_req_model + ";";
} else {
if (ed_req_make) {
params += "make=" + ed_req_make + ";";
}
if (ed_req_model) {
params += "mdl=" + ed_req_model + ";";
}
}
for (var i = 0; i < ed_req_category_list.length; i++) {
params += "theme=" + ed_req_category_list[i] + ";";
}
for (var i = 0; i < ed_req_type_list.length; i++) {
params += "type=" + ed_req_type_list[i] + ";";
}
return(params);
}

function ed_genUParams(adtype) {
var params = "";
if (ed_req_make) {
params += ":make_" + ed_req_make;
}
if (ed_req_model) {
params += ":mdl_" + ed_req_model;
}
for (var i = 0; i < ed_req_category_list.length; i++) {
params += ":theme_" + ed_req_category_list[i];
}
for (var i = 0; i < ed_req_type_list.length; i++) {
params += ":type_" + ed_req_type_list[i];
}
return(params);
}

//]]>
</script>

