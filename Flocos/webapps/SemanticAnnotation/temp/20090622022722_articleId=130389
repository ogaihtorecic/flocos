<script language="javascript">
YAHOO.util.Event.addListener(window, 'load', function() {
	YAHOO.Edmunds.LightBoxMedia.init();
	if (document.getElementById('gl-registration') != null && document.getElementById('gl-registration') != undefined){
		document.getElementById('gl-registration').style.display = 'block';
	}
	if (document.getElementById('regHeader') != null && document.getElementById('regHeader') != undefined){
		document.getElementById('regHeader').style.display = 'block';
	}
	if (YAHOO.Edmunds.Util.getQueryString('actionPerform') == 'reset-password') {
		YAHOO.Edmunds.LightBoxMedia.open(document.getElementById('reset-password-link'));
	}
	if (YAHOO.Edmunds.Util.getQueryString('actionPerform') == 'reset-password-link-error') {
	    YAHOO.Edmunds.LightBoxMedia.open(document.getElementById('reset-password-link-error'));
	}
	if (YAHOO.Edmunds.Util.getQueryString('actionPerform') == 'account-activation') {
	    YAHOO.Edmunds.LightBoxMedia.open(document.getElementById('account-activation'));
	}
});

// The js function takes three parameters.
// fromUrl : Current url
// toUrl : After login, where the user intends to go
// app : application name, e.g. insideline, autoobserver, carspace, default is edmunds
// The look and feel of the light box changes depending upon the app
function isLoggedIn(toUrl, app) {
    if(user.isLoggedIn() || user.isAutoLoggedIn()){
        if (toUrl != null && toUrl != undefined && toUrl != ''){
            window.location = toUrl;
        } else {
            window.location.reload();
        }		
     } else {
        var newLink=document.createElement('a');
         newLink.href= 'http://www.edmunds.com/era/secure/lb/login.jsp?fromUrl=' + window.location.href;
         if (toUrl != null && toUrl != undefined && toUrl != ''){
            newLink.href= newLink.href + '&toUrl=' + toUrl;
         }
         if (app != null && app != undefined && app != ''){
            newLink.href= newLink.href + '&app=' + app;
         } 
		newLink.className='option';
		newLink.rel='shadowbox;width=490;height=307';
		YAHOO.Edmunds.LightBoxMedia.open(newLink);	    
		return;
     }	 		    
}
 
function switchLightbox(href,rel,value){
     	  	var newLink=document.createElement('a');
	 			newLink.href= href + value;
	 			newLink.className='option';
	 			newLink.rel=rel;
	            YAHOO.Edmunds.LightBoxMedia.switchOverlay();
				YAHOO.Edmunds.LightBoxMedia.open(newLink);
				return;
	             
}

var x = function() { return YAHOO.Edmunds.LightBoxMedia.close;}();
YAHOO.Edmunds.LightBoxMedia.close = function() {
    new x();
	var loc = window.location.href;	
	if (loc.indexOf('email=') > 0){
		var pos = loc.indexOf('email=');
		loc = loc.substring(0,pos);		
		window.location = loc;
	} else if (loc.indexOf('?actionPerform=') > 0){
		var pos = loc.indexOf('?actionPerform=');
		loc = loc.substring(0,pos);		
		window.location = loc;
	}
}

function closeLightbox(){
         	YAHOO.Edmunds.LightBoxMedia.close();
}

YAHOO.util.Event.addListener('lightbox-close', 'click', function(e) {
             var e = e || window.event;
             YAHOO.util.Event.preventDefault(e);
             YAHOO.Edmunds.LightBox.hide();
})

         
</script>


